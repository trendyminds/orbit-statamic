{
    "name": "trendyminds/orbit-statamic",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Trendyminds\\Orbit\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Orbit",
            "description": "Orbit Statamic addon"
        },
        "laravel": {
            "providers": [
                "Trendyminds\\Orbit\\ServiceProvider"
            ]
        }
    },
    "version": "2.0.0",
    "require": {
        "php": "^8.2",
        "statamic/cms": "^4.0|^5.0"
    },
    "require-dev": {
        "laravel/pint": "^1.15"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
